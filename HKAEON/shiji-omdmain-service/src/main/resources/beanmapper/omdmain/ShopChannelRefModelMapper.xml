  <!-- 查询某个渠道下面的所有门店信息 -->
  <select id="queryShopsByChannel" parameterType="map" resultType="map">
  	select s.* from  shopchannelref r join shop s on r.shopId = s.shopId and r.entId = s.entId where r.channelCode =  #{channelCode}
  </select>
  
